<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
          <basicHttpBinding>
            <binding name="TestWSBinding">
              <security mode="TransportCredentialOnly">
                <transport clientCredentialType="Windows" />
              </security>
            </binding>
            <binding name="ProfielOverzicht_Binding">
            <security mode="TransportCredentialOnly">
              <transport clientCredentialType="Windows" />
            </security>
            </binding>
          </basicHttpBinding>
        </bindings>
      <behaviors>
        <endpointBehaviors>
          <behavior name="allowDelegation">
            <clientCredentials>
              <windows allowedImpersonationLevel="Delegation"
                       allowNtlm="true"/>
            </clientCredentials>
          </behavior>
        </endpointBehaviors>
      </behaviors>
      <client>
        <endpoint address="http://dkp-appl-01.gebrdekoning.local:7047/NST100TestWS/WS/SystemService"
          behaviorConfiguration="allowDelegation" 
          binding="basicHttpBinding"
          bindingConfiguration="TestWSBinding" 
          contract="SystemServiceTestRef.SystemService_Port"
          name="SystemService_Port" />
        <endpoint address="http://dkp-appl-01.gebrdekoning.local:7047/NST100TestWS/WS/Contoso%20GWW%20bedrijf/Page/ProfielOverzicht"
          behaviorConfiguration="allowDelegation"
          binding="basicHttpBinding" 
          bindingConfiguration="ProfielOverzicht_Binding"
          contract="NST100TestContosoProfielOverzichtRef.ProfielOverzicht_Port"
          name="ProfielOverzicht_Port" />
      </client>
    </system.serviceModel>
</configuration>